    function require(src, callback) {
      var script = window.document.createElement('script');
      script.src = src;
      script.async = true;
      document.body.appendChild(script);
      if (callback && typeof(callback) === 'function') {
          script.onload = callback;
      }
      return script;
    }
  </script>

