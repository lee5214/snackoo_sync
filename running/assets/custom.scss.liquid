//popup
.popup-content{
    position: relative !important;
    padding: 5% 0 !important;
    margin: 50vh auto 0 !important;
  max-width: 400px;
  top:0!important;
}
@media (min-width: 769px){
  .popup-discount--image .popup-content {
    //position: relative !important;
  }
}

//footer
footer {
  border-radius: 44px;
  .wrapper{
    padding-bottom: 0 !important;
  }
  .footer__item--contact ul > li {
    padding-left: 24px;
    margin-bottom: 3px;
    i {
      left: 0;
    }
  }
  .footer__item--menu > .footer-list{
    padding-top: 16px;
    .footer-list__item {
      margin-bottom: 3px;
      a {
        padding: 0;

        &:hover {
          color: #f06060;
        }
      }
    }
  }

  .footer__item--social{
    @media (max-width: 768px){
      width: 100%;
    }
    h4{
      padding-left: 24px;
    }
    @media (max-width: 768px){
      h4{
        padding-left: 0;
        display: block !important;
        text-align: center;
      }
    }
    .social-icons--inverse {
      padding-top:10px;
      .social-icons__item {
        margin: 10px;
      }
    }
  }
  .subscribe-form--footer > .subscribe-form--footer{
    padding-top: 20px;
  }

  //footer_subscription
  @media (min-width: 481px) {
    #mc-embedded-subscribe {
      font-size: 14px;
      font-weight: 600;
      width: 100%;
      position: absolute;
      top: 40px;
      left: 0;
      color: #5c4b51;
      background-color: transparent;

      &:hover {
        color: #f06060;
      }
    }
    #mce-EMAIL{
      height: 30px;
    }
  }
}

//weixin
#shopify-section-footer > footer > div > div > div.grid__item.large--one-quarter.medium--one-half.one-whole.footer__item.footer__item--social > ul > .weixin{
  .weixin__img {
    width: 0;
    height: 0;
    top: 10px;
