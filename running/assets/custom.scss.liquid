}

//search bar
.header .search-container {
  left: auto;
  right: 160px;
  @media (min-width: 767px) and (max-width: 1023px){
  	//display: none;
  }
}
.header .search-container.open {
    .form--search-primary {
      .form__field {
        @media(max-width: 1023px) {
          	width: 50px;
        }
        @media (min-width: 1024px) and (max-width: 1300px) {
        	width: 100px;
        }
        @media(min-width: 1300px) and (max-width: 1599px) {
        	width: 200px;
        }
        @media(min-width: 1600px) {
        	width: 300px;
        }
      }
    }

}


//rating-star
.spr-badge{
  margin-bottom: 30px;
  text-align: center;
  .spr-icon {
    padding: 0 3px;
    &.spr-icon-star {
      color: #f3b562;
      &:before {
        font-size: 120%;
      }
    }
    &.spr-icon-star-empty {
      color: #d1cacc;
      &:before {
        font-size: 100%;
      }
    }
  }
}

