//for mobile only


@media (max-width: 768px) {
  .fixed-bar {
    display: none;
  }
  .header {
    .drawer {
      height: 60px;
      background: white;
      position: fixed;
      .drawer__btn-open {
        position: fixed;
        transform: translate3d(0, 0, 0) !important;
        top:20px;
      }
    }
    .site-logo--mobile{
      margin-top: 0 !important;
      position: fixed;
      z-index: 5;
      max-width: 200px;
    }
    .drawer__wrapper{
      .drawer__header{
        //padding: 0;
        .site-logo{
          display: none;
        }
      }
    }

    &.header--clear {
      height: 0 !important;
      &.fixed{
        .drawer {
          .drawer__btn-open {
          }
        }
      }
    }
  }
  .template-product {
    #container {
      margin-top: 30px;
    }
  }
  .drawer--menu .drawer__header>.store-desc{
    transform: translate3d(0, 0px, 0);
    margin:10px 0 !important;
  }
  #shopify-section-header > header > div.drawer.drawer--menu.open > div > ul > li.nav-list__item.nav-list__item--currency{
    display:none;
  }

}

@media (max-width: 1024px){
  #shopify-section-offers-section_1{
    .special-offer-container {
      a {
        width: 100vw;
        margin-left: 0;
        margin-bottom: 30px;
      }
    }
  }
}

//cart drawer
#cart{
    .drawer__wrapper{
      //top: 0;
      .cart{
        form {
          height: 80vh;
          overflow: scroll;
          & > .cart__checkout-btn > button{
            width: auto;
            border-radius: 50px;
          }
        }
        @media (max-width: 769px){
          padding: 4px;
        }
      }
    }
  }
//for pc only

//products catalog title
#shopify-section-header > header > div.drawer.drawer--menu.close > div > ul > li:nth-child(2) > ul > li > a{
  background: #a09c9c;
  width: 80%;
  display: block;
  margin:0 auto;
  color: white;
  font-family: raleway,"Bitter",serif;
  &:hover{
    background: white;
    color: #73656a;
    font-weight: 600;
    transition: all 200ms ease;
    .nav-list__title{
      color: white;
    }
  }
}
@media (min-width: 769px) {
  #shopify-section-header{
    .drawer{
      .drawer__wrapper{
        .nav-list{
          //products catalog subtitle (min secs)
          & > .has-dropdown>.nav-list__dropdown>.has-dropdown>.nav-list__dropdown .nav-list__link:hover{
            color: #73656a;
            font-weight: 600;
            transition: all 200ms ease;
          }
          /*li:nth-child(2) > ul > li > a{
            background: #a09c9c;
